    "deploy:scroll-sepolia:soulwallet": "source .env && forge script script/WalletDeployer.s.sol:WalletDeployer --ffi --slow --rpc-url scrollsepolia --broadcast --verify --legacy",
    "deploy:scroll-sepolia:paymaster": "source .env && forge script script/PaymasterDeployer.s.sol:PaymasterDeployer --ffi --slow --rpc-url scrollsepolia --broadcast --verify --legacy",
    "deploy:scroll-sepolia:all": "npm run deploy:scroll-sepolia:keystore && npm run deploy:scroll-sepolia:soulwallet && npm run deploy:scroll-sepolia:paymaster",
    "deploy:base-sepolia:keystore": "source .env && forge script script/KeyStoreDeployer.s.sol:KeyStoreDeployer --ffi --slow --rpc-url $BASE_SEPOLIA_RPC_URL --broadcast --verify --etherscan-api-key $BASE_SEPOLIA_SCAN_API_KEY",
