    "deploy:op-goerli:all": "npm run deploy:op-goerli:keystore && npm run deploy:op-goerli:soulwallet && npm run deploy:op-goerli:paymaster",
    "deploy:arb-goerli:all": "npm run deploy:arb-goerli:keystore && npm run deploy:arb-goerli:soulwallet && npm run deploy:arb-goerli:paymaster",
    "deploy:all": "npm run deploy:goeril:all && npm run deploy:op-goerli:all && npm run deploy:arb-goerli:all",
